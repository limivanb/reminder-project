<p>Total: {{count}}</p>
<div id="event_list">
  <table class="ui single line table">

    <thead>
      <tr>
        <th></th>
        <th>Date</th>
        <th>Activity Name</th>
        <th>Location</th>
      </tr>
    </thead>
    <tbody>
      {{#event_list}}
      <tr>
          <td class="collapsing">
            <div class="ui fitted checkbox">
              <input id="{{_id}}" name="selectedEvent[]" type="checkbox"> <label></label>
            </div>
          </td>
          <td>{{dateFormat event_date "dddd, MMMM Do YYYY, h:mm a"}}</td>
          <td>{{event_name}}</td>
          <td>{{location}}</td>
      </tr>
      {{/event_list}}
    </tbody>

  </table>
</div>
